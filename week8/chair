package week_8;

public class Chair 
{
	String manufacturer = "";
	String model = "";
	String color ="";
	double mfgCost = 0; 
	double retailPrice =0;
	int itemWeight =0;
	
	public Chair(String manufacturer, String model, 
			String color, int itemWeight,
			double mfgCost, double retailPrice) {
	super();
	this.manufacturer = manufacturer;
	this.model = model;
	this.color = color;
	this.itemWeight = itemWeight;
	this.mfgCost = mfgCost;
	this.retailPrice = retailPrice;
	
}
public String getManufacturer()	{
	return manufacturer;
}
public void setManufacturer(String manufacturer) {
	this.manufacturer = manufacturer;
}

public String getModel() {
	return model;
}

public void setModel(String model) {
	this.model = model;
}

public String getColor() {
	return color;
}

public void color(String color) {
	this.color = color;
}
public int getItemWeight() {
	return itemWeight;
}

public void setItemWeight(int itemWeight) {
	this.itemWeight = itemWeight;
}

public double getMfgCost() {
	return mfgCost;
}

public void setMfgCost(float mfgCost) {
	this.mfgCost = mfgCost;
}

public double getRetailPrice() {
	return retailPrice;
}

public void setRetailPrice(float retailPrice) {
	this.retailPrice = retailPrice;
}
public String toString() {
	return "Chair: \\n\\tmanufacturer = \" + manufacturer + \r\n"
			+ "				\",\\n\\tmodel = \" + model + \r\n"
			+ "			    \",\\n\\tcolor = \" + color + \r\n"
			+ "				\",\\n\\titemWeight = \" + itemWeight + "
			+ "				\",\\n\\tmfgCost = \" + mfgCost + \r\n"
			+ "				\",\\n\\tretailPrice = \" + retailPrice +\r\n";
	
}
}
